*MODULE MAIN
  DO;
    /* This is a comment */
    CALL PUTCHAR ('H');
    CALL PUTCHAR ('e');
    CALL PUTCHAR ('l');
    CALL PUTCHAR ('l');
    CALL PUTCHAR ('o');
    CALL PUTCHAR (' ');
    CALL PUTCHAR ('W');
    CALL PUTCHAR ('o');
    CALL PUTCHAR ('r');
    CALL PUTCHAR ('l');
    CALL PUTCHAR ('d');
    CALL PUTCHAR ('!');
    CALL PUTCHAR (13); /* carriage return */
    CALL PUTCHAR (10); /* line feed */
  END;
END MAIN;